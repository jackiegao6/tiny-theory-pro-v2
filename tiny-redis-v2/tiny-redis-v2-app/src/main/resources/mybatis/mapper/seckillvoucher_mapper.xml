<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gzc.infrastructure.dao.ISecKillVoucherDao">

    <resultMap id="dataMap" type="com.gzc.infrastructure.dao.po.SeckillVoucher">
        <result column="voucher_id" property="voucherId"/>
        <result column="stock" property="stock"/>
        <result column="begin_time" property="beginTime"/>
        <result column="end_time" property="endTime"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <insert id="insert" parameterType="com.gzc.infrastructure.dao.po.SeckillVoucher">
        insert into tb_seckill_voucher (
            voucher_id, stock,
            begin_time, end_time, create_time, update_time
        )
        values (
                #{voucherId},
                #{stock},
                #{beginTime}, #{endTime}, now(), now()
               )
    </insert>

</mapper>
